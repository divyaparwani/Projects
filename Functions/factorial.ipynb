    "def factorial(num):\n",
    "    temp=1\n",
    "    while num>0:\n",
    "        temp=temp*num\n",
    "        num=num-1\n",
    "    return temp     \n",
    "num=int(input(\"Enter a no.:\"))\n",
    "res=factorial(num)\n",
    "print(res)"
   ]
