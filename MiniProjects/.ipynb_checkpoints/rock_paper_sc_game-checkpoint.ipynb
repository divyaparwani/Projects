    "from getpass import getpass\n",
    "a=getpass(\"Player 1:\")\n",
    "b=getpass(\"Player 2:\")\n",
    "ch=(a,b)\n",
    "p1=(('s','p'),('r','s'),('p','r'))\n",
    "if a==b:\n",
    "    print(\"Match tie\")\n",
    "elif ch in p1:\n",
    "    print(\"player 1 won\")\n",
    "else:\n",
    "    print(\"player 2 won\")\n",
    "    \n"
